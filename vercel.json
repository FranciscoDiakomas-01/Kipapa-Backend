{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs23.x",
        "installCommand": "npm ci"
      }
    }
  ],
  "routes": [
    {
      "src": "/kipapa/(.*)",
      "dest": "/kipapa/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/.next/$1" 
    }
  ]
}
